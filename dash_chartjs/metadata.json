{
  "src/lib/components/ChartJs.react.js": {
    "description": "This component renders ChartJs React component inside Dash App.",
    "displayName": "ChartJs",
    "methods": [],
    "props": {
      "id": {
        "type": {
          "name": "string"
        },
        "required": false,
        "description": "The ID used to identify this component in Dash callbacks."
      },
      "type": {
        "type": {
          "name": "string"
        },
        "required": false,
        "description": "Chart.js chart type."
      },
      "data": {
        "type": {
          "name": "object"
        },
        "required": false,
        "description": "The data object that is passed into the Chart.js chart.",
        "defaultValue": {
          "value": "{\r\n    datasets: []\r\n}",
          "computed": false
        }
      },
      "options": {
        "type": {
          "name": "object"
        },
        "required": false,
        "description": "The options object that is passed into the Chart.js chart.",
        "defaultValue": {
          "value": "{}",
          "computed": false
        }
      },
      "toolbox": {
        "type": {
          "name": "bool"
        },
        "required": false,
        "description": "Toolbox with reset and download buttons for chart.",
        "defaultValue": {
          "value": "true",
          "computed": false
        }
      },
      "linearGradientList": {
        "type": {
          "name": "array"
        },
        "required": false,
        "description": "List of colors for the Linear Gradient.",
        "defaultValue": {
          "value": "[]",
          "computed": false
        }
      },
      "linearGradientDirection": {
        "type": {
          "name": "enum",
          "value": [
            {
              "value": "'vertical'",
              "computed": false
            },
            {
              "value": "'horizontal'",
              "computed": false
            }
          ]
        },
        "required": false,
        "description": "Set the direction of Linear Gradient. Either 'horizontal' or 'vertical'. Vertical is default.",
        "defaultValue": {
          "value": "'vertical'",
          "computed": false
        }
      },
      "useGradient": {
        "type": {
          "name": "enum",
          "value": [
            {
              "value": "'borderColor'",
              "computed": false
            },
            {
              "value": "'backgroundColor'",
              "computed": false
            },
            {
              "value": "'both'",
              "computed": false
            }
          ]
        },
        "required": false,
        "description": "Apply Linear Gradient on 'borderColor', 'backgroundColor' or on 'both'. Applies on both by default.",
        "defaultValue": {
          "value": "'both'",
          "computed": false
        }
      },
      "clickData": {
        "type": {
          "name": "object"
        },
        "required": false,
        "description": "clickData returns the datasetIndex and index of data point clicked.",
        "defaultValue": {
          "value": "{}",
          "computed": false
        }
      },
      "style": {
        "type": {
          "name": "object"
        },
        "required": false,
        "description": "Defines CSS styles which will override styles previously set."
      },
      "setProps": {
        "type": {
          "name": "func"
        },
        "required": false,
        "description": "Dash-assigned callback that should be called to report property changes\r\nto Dash, to make them available for callbacks."
      }
    }
  }
}